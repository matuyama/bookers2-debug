<% if user_signed_in? %>
  <div class="search_form">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
    <teble>
      <tr>
      <th><%= f.text_field :word %></th>
      <th><%= f.select :range, options_for_select([['User'], ['Book']]) %></th>
      <th><%= f.select :search, options_for_select([["完全一致", "perfect_match"], ["前方一致", "forwaed_match"], ["後方一致", "backwerd_match"], ["部分一致", "partial_match"]]) %></th>
      <th><%= f.submit "検索", class: "btn btn-primary" %></th>
      </tr>
    </teble>
    <% end %>
  </div>
<% end %>